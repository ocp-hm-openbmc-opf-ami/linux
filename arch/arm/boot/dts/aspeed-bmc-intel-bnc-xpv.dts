	hub@70,3C000000100 {
		reg = <0x70 0x3C0 0x00000100>;
		assigned-address = <0x70>;
		dcr = <0xC2>;

		cp0-ldo = "disabled";
		cp1-ldo = "disabled";
		tp0145-ldo = "disabled";
		tp2367-ldo = "disabled";
