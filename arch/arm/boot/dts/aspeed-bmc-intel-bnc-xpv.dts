/dts-v1/;

#include "aspeed-bmc-intel-bhs.dtsi"

/ {
	model = "Intel Beechnut City xPV BMC";
	compatible = "intel,bnc-xpv-bmc", "aspeed,ast2600";
};

&mac3 {
	/* OCP NIC is not supported obn xPV platforms */
	status = "disabled";
};

&i3c2 {
	/* I3C_MNG_LVC18 */

	hub@0,0 {
		cp0-ldo = "disabled";
		cp1-ldo = "disabled";
		tp0145-ldo = "disabled";
		tp2367-ldo = "disabled";
		tp0145-pullup = "2k";
		tp2367-pullup = "2k";

		target-port@0 {
			mode = "i3c";
			pullup = "enabled";
		};
		target-port@1 {
			mode = "disabled";
			pullup = "enabled";
		};
		target-port@2 {
			mode = "i3c";
			pullup = "enabled";
		};
		target-port@3 {
			mode = "disabled";
			pullup = "enabled";
		};
		target-port@4 {
			mode = "disabled";
			pullup = "enabled";
		};
		target-port@5 {
			mode = "disabled";
			pullup = "enabled";
		};
		target-port@6 {
			mode = "disabled";
			pullup = "enabled";
		};
		target-port@7 {
			mode = "disabled";
			pullup = "enabled";
		};
	};
};

